[uwsgi]
#application's base folder
base = {{ vagrant_control_home }}/{{ vagrant_control_application_name }}
processes = 4
app = vagrantControl
module = %(app)
pythonpath = %(base)
socket = {{ vagrant_control_socket_path }}
chmod-socket    = 666
callable = app
logto = /var/log/{{ vagrant_control_application_name }}/%n.log
workers = 10
python-autoreload = 1
